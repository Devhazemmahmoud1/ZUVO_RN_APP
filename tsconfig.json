{
  "compilerOptions": {
    "baseUrl": ".",
  "paths": {
  "*": ["node_modules/*"],
  },
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-native",
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": ["react", "react-native", "@types/react"],
    "noImplicitAny": false
  },
  "exclude": ["node_modules", "@react-native/typescript-config"],
  "include": ["src"]
}
